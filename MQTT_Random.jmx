              <stringProp name="script">// Initialize a custom iteration counter if not already set
if (vars.get(&quot;customIteration&quot;) == null) {
    vars.put(&quot;customIteration&quot;, &quot;1&quot;); // Start at 1
} else {
    // Increment the counter for each loop
    int iteration = vars.get(&quot;customIteration&quot;).toInteger() + 1;
    vars.put(&quot;customIteration&quot;, String.valueOf(iteration));
}
// Generate dynamic values using the custom iteration count
vars.put(&quot;txnAmt&quot;, String.valueOf(100 + (vars.get(&quot;customIteration&quot;).toInteger() * 50))); // Increment amount

